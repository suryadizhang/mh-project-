{
  "name": "myhibachi-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "next build",
    "check:css-conflicts": "stylelint \"**/*.{css,scss}\" --config .stylelintrc",
    "check:styles": "npm run lint:css",
    "dev": "next dev",
    "dev:stable": "next dev",
    "dev:turbo": "next dev --turbopack",
    "format": "prettier --write . && sort-package-json package.json",
    "lint": "next lint --max-warnings=0",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    ">=0.5%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@my-hibachi/blog-types": "file:../../packages/blog-types",
    "@radix-ui/react-slot": "^1.2.3",
