{
  "name": "@myhibachi/api-client",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./booking": "./src/booking/index.ts",
    "./auth": "./src/auth/index.ts",
    "./admin": "./src/admin/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "generate": "openapi-typescript ../api/openapi.json -o src/types/api.ts",
    "generate:watch": "npm run generate -- --watch"
  },
  "dependencies": {
    "jspdf": "4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.0.0"
  }
}
