{
  "name": "myhibachi-workspace",
  "version": "1.0.0",
  "description": "MyHibachi - Full-Stack Booking System with Payments and AI",
  "private": true,
  "author": "MyHibachi Team",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/suryadizhang/mh-project-.git"
  },
  "workspaces": [
    "myhibachi-frontend",
    "myhibachi-admin-frontend",
    "myhibachi-backend"
  ],
  "scripts": {
    "dev:customer": "cd myhibachi-frontend && npm run dev",
    "dev:admin": "cd myhibachi-admin-frontend && npm run dev",
    "dev:backend": "cd myhibachi-backend && python -m uvicorn main:app --reload --port 8000",
    "dev:ai": "cd myhibachi-ai-backend && python main.py",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:customer\"",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:customer\" \"npm run dev:admin\" \"npm run dev:ai\"",
    "build": "npm run build:customer && npm run build:admin",
    "build:customer": "cd myhibachi-frontend && npm run build",
    "build:admin": "cd myhibachi-admin-frontend && npm run build",
    "start": "npm run start:customer",
    "start:customer": "cd myhibachi-frontend && npm start",
    "start:admin": "cd myhibachi-admin-frontend && npm start",
    "lint": "npm run lint:customer && npm run lint:admin && npm run lint:backend",
    "lint:customer": "cd myhibachi-frontend && npm run lint",
    "lint:admin": "cd myhibachi-admin-frontend && npm run lint",
    "lint:backend": "ruff check . && black --check .",
    "format": "npm run format:customer && npm run format:admin && npm run format:backend",
    "format:customer": "cd myhibachi-frontend && npm run format",
    "format:admin": "cd myhibachi-admin-frontend && npm run format",
    "format:backend": "black . && isort .",
    "type-check": "npm run type-check:customer && npm run type-check:admin",
    "type-check:customer": "cd myhibachi-frontend && npm run typecheck",
    "type-check:admin": "cd myhibachi-admin-frontend && npm run typecheck",
    "test": "npm run test:customer && npm run test:admin && npm run test:backend",
    "test:customer": "cd myhibachi-frontend && npm test",
    "test:admin": "cd myhibachi-admin-frontend && npm test",
    "test:backend": "cd myhibachi-backend && pytest",
    "test:e2e": "npx playwright test",
    "clean": "rm -rf node_modules && cd myhibachi-frontend && rm -rf node_modules .next && cd ../myhibachi-admin-frontend && rm -rf node_modules .next"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0",
    "python": ">=3.9.0"
  },
  "keywords": [
    "hibachi",
    "catering",
    "booking-system",
    "nextjs",
    "fastapi",
    "stripe",
    "payments",
    "restaurant",
    "scheduling"
  ]
}
