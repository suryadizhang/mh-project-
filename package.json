{
  "name": "myhibachi-workspace",
  "version": "1.0.0",
  "description": "MyHibachi - Full-Stack Booking System with Payments and AI",
  "private": true,
  "author": "MyHibachi Team",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/suryadizhang/mh-project-.git"
  },
  "workspaces": [
    "myhibachi-frontend",
    "myhibachi-backend"
  ],
  "scripts": {
    "dev:frontend": "cd myhibachi-frontend && npm run dev",
    "dev:backend": "cd myhibachi-backend && python -m uvicorn main:app --reload --port 8000",
    "dev:ai": "cd myhibachi-ai-backend && python main.py",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:ai\"",
    "build": "npm run build:frontend",
    "build:frontend": "cd myhibachi-frontend && npm run build",
    "start": "npm run start:frontend",
    "start:frontend": "cd myhibachi-frontend && npm start",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd myhibachi-frontend && npm run lint",
    "lint:backend": "ruff check . && black --check .",
    "format": "npm run format:frontend && npm run format:backend",
    "format:frontend": "cd myhibachi-frontend && npm run format",
    "format:backend": "black . && isort .",
    "type-check": "cd myhibachi-frontend && npm run type-check",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd myhibachi-frontend && npm test",
    "test:backend": "cd myhibachi-backend && pytest",
    "clean": "rm -rf node_modules && cd myhibachi-frontend && rm -rf node_modules .next",
    "postinstall": "cd myhibachi-frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0",
    "python": ">=3.9.0"
  },
  "keywords": [
    "hibachi",
    "catering",
    "booking-system",
    "nextjs",
    "fastapi",
    "stripe",
    "payments",
    "restaurant",
    "scheduling"
  ]
}
